<test>
    <SQLqueries>
        <!-- FINANCE EDITION START -->
        <!-- instruments -->
        <query>
            <testID>fin-1-symbol</testID>
            <queryText>
                <![CDATA[
                SELECT entity FROM atsd_entity WHERE tags.symbol  = 'GAZP'
                ]]>
            </queryText>
            <loops>20</loops>
        </query>
        
    </SQLqueries>

    <dataQueries>

        <dataQuery>
            <testID>data--fin-detail-1-symbol-ASC</testID>
            <payload>
                <![CDATA[
                {
                    "entity": "gazp_[tqbr]",
                    "metric": "trade_price",
                    "startDate": "2020-08-20T00:00:00.000Z",
                    "endDate":   "2020-08-27T00:00:00.000Z"
                }
                ]]>
            </payload>
            <path>/api/v1/series/query</path>
            <method>POST</method>
            <loops>20</loops>
        </dataQuery>

        <!-- instrument search -->
        <dataQuery>
            <testID>data--fin-instr-search-1</testID>
            <path>/financial/trades/suggest/instruments?pattern=g&#38;className=&#38;limit=20</path>
            <method>GET</method>
            <payload></payload>
            <loops>20</loops>
        </dataQuery>
 
        <dataQuery>
            <testID>data--entity-search-docker-1</testID>
            <path>/api/v1/entities?addInsertTime=false&#38;expression=tags.docker-type = 'image'</path>
            <method>GET</method>
            <payload></payload>
            <loops>10</loops>
        </dataQuery>
 
        <dataQuery>
            <testID>data--entity-search-fin-1</testID>
            <path>/api/v1/entities?addInsertTime=false&#38;expression=tags.class_code = 'TQBR'</path>
            <method>GET</method>
            <payload></payload>
            <loops>10</loops>
        </dataQuery>

        <!--Raw trades export-->
        <dataQuery>
            <testID>gh-tsd-1054-raw-1</testID>
            <path>/api/v1/trades?symbol=GAZP&#38;class=TQBR&#38;startDate=2021-01-04&#38;endDate=2021-01-05&#38;exchange=moex</path>
            <method>GET</method>
            <payload></payload>
            <loops>10</loops>
        </dataQuery>
 
    </dataQueries>
</test>
