<test>
    <dataQueries>
        <dataQuery>
            <testID>sql-time-formatter-test-time-raw-single-column</testID>
            <payload>
                <![CDATA[
                q=SELECT time FROM "mpstat.cpu_busy"
                ]]>
            </payload>
            <path>/api/sql?discardOutput=true</path>
            <method>POST</method>
            <loops>10</loops>
        </dataQuery>
        <dataQuery>
            <testID>sql-time-formatter-test-time-raw</testID>
            <payload>
                <![CDATA[
                q=SELECT time, time, time, time, time FROM "mpstat.cpu_busy"
                ]]>
            </payload>
            <path>/api/sql?discardOutput=true</path>
            <method>POST</method>
            <loops>10</loops>
        </dataQuery>
        <dataQuery>
            <testID>sql-time-formatter-test-datetime</testID>
            <payload>
                <![CDATA[
                q=SELECT datetime, datetime, datetime, datetime, datetime FROM "mpstat.cpu_busy"
                ]]>
            </payload>
            <path>/api/sql?discardOutput=true</path>
            <method>POST</method>
            <loops>10</loops>
        </dataQuery>
        <dataQuery>
            <testID>sql-time-formatter-test-iso-utc</testID>
            <payload>
                <![CDATA[
                q=SELECT date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSS''Z''', 'UTC'), date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSS''Z''', 'UTC'), date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSS''Z''', 'UTC') FROM "mpstat.cpu_busy"
                ]]>
            </payload>
            <path>/api/sql?discardOutput=true</path>
            <method>POST</method>
            <loops>10</loops>
        </dataQuery>
        <dataQuery>
            <testID>sql-time-formatter-test-iso-local</testID>
            <payload>
                <![CDATA[
                q=SELECT date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSSXXX'), date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSSXXX'), date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSSXXX'), date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSSXXX'), date_format(time, 'yyyy-MM-dd''T''HH:mm:ss.SSSXXX') FROM "mpstat.cpu_busy"
                ]]>
            </payload>
            <path>/api/sql?discardOutput=true</path>
            <method>POST</method>
            <loops>10</loops>
        </dataQuery>
        <dataQuery>
            <testID>sql-time-formatter-test-us-date</testID>
            <payload>
                <![CDATA[
                q=SELECT date_format(time, 'MM/dd/yyyy'), date_format(time, 'MM/dd/yyyy'), date_format(time, 'MM/dd/yyyy'), date_format(time, 'MM/dd/yyyy'), date_format(time, 'MM/dd/yyyy') FROM "mpstat.cpu_busy"
                ]]>
            </payload>
            <path>/api/sql?discardOutput=true</path>
            <method>POST</method>
            <loops>10</loops>
        </dataQuery>
    </dataQueries>
</test>